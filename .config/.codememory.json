{
    "individual-tasks": {
        "pathWithTasks": "Tasks",
        "namespaceWithTasks": "App",
        "tableWithTasks": "codememory_jobs",
        "tableWithInfo": "codememory_work",
        "configName": "job"
    },
    "caching": {
        "fileConfig": "configs\/caching"
    },
    "configuration": {
        "pathWithConfigs": "configs\/",
        "mode": "development"
    },
    "database-pack": {
        "defaultHost": "localhost",
        "defaultUsername": "root",
        "defaultPassword": "",
        "defaultDbname": "mysql",
        "defaultDriver": "Codememory\\Components\\Database\\Connection\\Drivers\\MySQLDriver",
        "configName": "database"
    },
    "environment": {
        "pathWithFile": null,
        "filename": ".env",
        "defaultAppMode": "dev",
        "useCache": false
    },
    "http": {
        "configName": "http"
    },
    "controlHttpStatus": {
        "configName": "httpStatus"
    },
    "migrations": {
        "pathWithMigrations": "Migrations\/",
        "namespaceMigration": "Migrations\\",
        "configName": "database"
    },
    "orm": {
        "pathWithEntities": "App\/Entity",
        "entityNamespace": "App\\Entity\\",
        "pathWithRepositories": "app\/Repositories",
        "repositoryNamespace": "App\\Repositories\\",
        "entitySuffix": "Entity",
        "repositorySuffix": "Repository",
        "configName": "database"
    },
    "profiler": {
        "configName": "profiling",
        "mode": "dev",
        "enabled": true,
        "security": {
            "subdomain": "profiler",
            "enabledInProduction": false
        }
    },
    "routing": {
        "configName": "routing",
        "settings": {
            "pathWithRoutes": "App\/Routing\/Routes",
            "softwareNamespace": "App\\Routing\\Software",
            "routesFileSuffix": "-routes"
        }
    },
    "serviceProvider": {
        "configName": "services"
    },
    "view": {
        "configName": "view",
        "pathWithTemplates": "\/",
        "engine": "Codememory\\Components\\View\\Engines\\BigEngine"
    }
}